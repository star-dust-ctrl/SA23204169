library(mvtnorm)
set.seed(1)
covmatrix <- matrix(0.3, ncol = 5, nrow = 5)
diag(covmatrix) <- 1
meanvalue <- rep(0,5)
X <- rmvnorm(100, meanvalue, covmatrix)
beta <- runif(5, min = 0.5, max = 1)
mu <- sample(c(-2, 2), size = 100, replace = TRUE)
eps <- rnorm(100, 0, 0.5)
y <- as.vector(X %*% beta) + mu + eps
SubgroupSolver(X,y)
